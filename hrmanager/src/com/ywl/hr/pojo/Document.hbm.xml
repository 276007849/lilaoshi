<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ywl.hr.pojo" default-lazy="false">
	<class name="Document" table="document_inf" >
		<id name="id">
			<generator class="native"></generator>
		</id>
		<property name="title"  type="java.lang.String" length="50"></property>
		<property name="fileName" column="filename" type="java.lang.String" length="300"></property>
		<property name="pemark"  type="java.lang.String" length="300"></property>
		<property name="createDate" column="create_date" ></property>
		<many-to-one name="user" class="User" column="user_id" cascade="save-update" lazy="false"></many-to-one>
	</class>
</hibernate-mapping>