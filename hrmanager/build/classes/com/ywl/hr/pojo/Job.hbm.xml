<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ywl.hr.pojo" default-lazy="false">
	<class name="Job" table="job_inf" >
		<id name="id">
			<generator class="native"></generator>
		</id>
		<property name="name"  type="java.lang.String" length="50"></property>
		<property name="pemark"  type="java.lang.String" length="300"></property>
	</class>
</hibernate-mapping>